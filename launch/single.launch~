  <param name="eta" value="$(arg eta)"/>
  <param name="map_topic" value="/robot_1/map"/>
  <param name="robot_frame" value="/robot_1/base_link"/>
  <!--param name="map_topic" value="/robot_1/map"/-->
  </node>

  <!--node pkg="rrt_exploration" type="assigner.py" name="assigner">
